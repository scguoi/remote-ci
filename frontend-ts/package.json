{
  "name": "frontend-ts",
  "version": "1.0.0",
  "description": "Frontend TypeScript Login Project",
  "type": "module",
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,html,css}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "type-check": "tsc --noEmit",
    "type-check:ci": "tsc --project tsconfig.ci.json --noEmit",
    "quality": "npm run lint && npm run type-check"
  },
  "dependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "vite": "^5.0.0"
  }
}
