{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "types": []
  },
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "coverage",
    "**/*.test.ts",
    "**/*.test.tsx",
    "src/__tests__/**/*",
    "jest.config.ts",
    "vite.config.ts"
  ],
  "typeAcquisition": {
    "enable": false
  }
}
